{
  "name": "@spotify/lighthouse-audit-service",
  "description": "Run, store, and view Lighthouse audits on all of your websites",
  "author": "Paul Marbach",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spotify/lighthouse-audit-service.git"
  },
  "license": "Apache-2.0",
  "version": "0.0.0-development",
  "private": true,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types",
  "scripts": {
    "dev": "nodemon",
    "start": "node ./cjs/run.js",
    "lint": "web-scripts lint",
    "test": "web-scripts test",
    "build": "web-scripts build && copyfiles --up 1 src/**/*.sql cjs && copyfiles --up 1 src/**/*.sql esm",
    "commit": "web-scripts commit",
    "release": "web-scripts release",
    "prepare": "web-scripts audit"
  },
  "husky": {
    "hooks": {
      "commit-msg": "web-scripts commitmsg",
      "pre-commit": "web-scripts precommit"
    }
  },
  "devDependencies": {
    "@spotify/web-scripts": "^6.1.0",
    "@types/async-retry": "^1.4.1",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.3",
    "@types/pg": "^7.14.3",
    "@types/puppeteer": "^2.0.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.2",
    "@types/wait-on": "^4.0.0",
    "copyfiles": "^2.2.0",
    "dotenv": "^8.2.0",
    "get-port": "^5.1.1",
    "husky": "^4.0.0",
    "node-duration": "^1.0.4",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2",
    "testcontainers": "^2.6.0",
    "ts-node": "^8.8.1"
  },
  "dependencies": {
    "async-retry": "^1.3.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "lighthouse": "^5.6.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "pg": "^7.18.2",
    "puppeteer": "^2.1.1",
    "sql-template-strings": "^2.2.2",
    "uuid": "^7.0.2",
    "wait-on": "^4.0.1",
    "winston": "^3.2.1"
  },
  "engines": {
    "node": ">=12"
  }
}
